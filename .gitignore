function data() {
	var date = new Date();
	let days = date.getDate();
	let month = date.getMonth() + 1;
	if (month < 10) month = "0" + month;
	if (days < 10) days = "0" + days;
	var datas = days + ":" + month + ":2022";
	return datas;
}

